fn main() {
    let mut _AAAAAAA: i16;
    let mut _BBBBBBB: i16;
    let mut _CCCCCCC: i16;
    let mut _MAXMAXM: i16;

    _AAAAAAA = input_i16();
    _BBBBBBB = input_i16();
    _CCCCCCC = input_i16();

    if _AAAAAAA >= _BBBBBBB && _AAAAAAA >= _CCCCCCC {
        _MAXMAXM = _AAAAAAA;
    } else if _BBBBBBB >= _AAAAAAA && _BBBBBBB >= _CCCCCCC {
        _MAXMAXM = _BBBBBBB;
    } else {
        _MAXMAXM = _CCCCCCC;
    }
    println!("{}", _MAXMAXM);

    if (_AAAAAAA == _BBBBBBB) && (_AAAAAAA == _CCCCCCC) && (_BBBBBBB == _CCCCCCC) {
        println!("{}", 1);
    } else {
        println!("{}", 0);
    }

    if !(_AAAAAAA >= 0) || !(_BBBBBBB >= 0) || !(_CCCCCCC >= 0) {
        println!("{}", -1);
    } else {
        println!("{}", 0);
    }

    if _AAAAAAA >= (_BBBBBBB + _CCCCCCC) {
        println!("{}", 10);
    } else {
        println!("{}", 0);
    }
}

fn input_i16() -> i16 {
    use std::io;
    let mut s = String::new();
    io::stdin().read_line(&mut s).unwrap();
    s.trim().parse::<i16>().unwrap()
}
