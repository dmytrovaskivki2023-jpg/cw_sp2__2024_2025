NAME _PROGRAM ;

BODY DATA
  INTEGER16 _AAAAAAA , _BBBBBBB , _XXXXXXX , _YYYYYYY ;

  /* зчитування двох чисел */
  GET ( _AAAAAAA )
  GET ( _BBBBBBB )

  /* виведення результатів арифметичних операцій */
  PUT ( _AAAAAAA + _BBBBBBB )
  PUT ( _AAAAAAA - _BBBBBBB )
  PUT ( _AAAAAAA * _BBBBBBB )
  PUT ( _AAAAAAA DIV _BBBBBBB )
  PUT ( _AAAAAAA MOD _BBBBBBB )

  /* обчислення проміжних змінних */
  ( _AAAAAAA - _BBBBBBB ) * 10 + ( _AAAAAAA + _BBBBBBB ) DIV 10 =: _XXXXXXX
  _XXXXXXX + _XXXXXXX MOD 10 =: _YYYYYYY

  /* виведення фінальних результатів */
  PUT ( _XXXXXXX )
  PUT ( _YYYYYYY )

END
