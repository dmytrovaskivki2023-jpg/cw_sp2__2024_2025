fn main() {
    let mut _AAAAAAA: i16;
    let mut _BBBBBBB: i16;
    let mut _XXXXXXX: i16;
    let mut _YYYYYYY: i16;

    println!("Введіть A:");
    _AAAAAAA = input_i16();

    println!("Введіть B:");
    _BBBBBBB = input_i16();

    println!("{}", _AAAAAAA + _BBBBBBB);
    println!("{}", _AAAAAAA - _BBBBBBB);
    println!("{}", _AAAAAAA * _BBBBBBB);
    println!("{}", _AAAAAAA / _BBBBBBB);
    println!("{}", _AAAAAAA % _BBBBBBB);

    _XXXXXXX = (_AAAAAAA - _BBBBBBB) * 10 + (_AAAAAAA + _BBBBBBB) / 10;
    _YYYYYYY = _XXXXXXX + (_XXXXXXX % 10);

    println!("{}", _XXXXXXX);
    println!("{}", _YYYYYYY);
}

fn input_i16() -> i16 {
    use std::io;
    let mut s = String::new();
    io::stdin().read_line(&mut s).unwrap();
    s.trim().parse::<i16>().unwrap()
}
